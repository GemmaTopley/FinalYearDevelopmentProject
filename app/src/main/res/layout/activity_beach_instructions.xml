<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/messysealevel"
    android:backgroundTintMode="src_over"
    tools:context=".BeachInstructions">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/seagulmascot" />

    <Button
        android:id="@+id/goBeachBtn"
        android:layout_width="315dp"
        android:layout_height="213dp"
        android:backgroundTint="#42A5F5"
        android:fontFamily="@font/comic_neue_light"
        android:onClick="startBtn"
        android:text="GO TO THE BEACH"
        android:textColor="@color/white"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="939dp"
        android:layout_height="473dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/beachspeechintro" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="828dp"
        android:layout_height="441dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/beachinstrucitons" />

</androidx.constraintlayout.widget.ConstraintLayout>