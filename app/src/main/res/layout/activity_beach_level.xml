<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sealevelback"
    android:onClick="litterBtn"
    tools:context=".BeachLevel">

    <ImageButton
        android:id="@+id/backBtn"
        android:layout_width="150dp"
        android:layout_height="75dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/backcloud"
        android:onClick="backBtn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/bottleBtn"
        android:layout_width="145dp"
        android:layout_height="176dp"
        android:layout_marginStart="39dp"
        android:layout_marginBottom="3dp"
        android:background="@drawable/greenplasticbottle"
        android:onClick="litterBtn"
        android:rotation="90"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/beerHolderBtn"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:layout_marginEnd="270dp"
        android:layout_marginBottom="270dp"
        android:background="@drawable/beerholder"
        android:onClick="litterBtn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/canBtn" />

    <ImageButton
        android:id="@+id/canBtn"
        android:layout_width="172dp"
        android:layout_height="128dp"
        android:layout_marginEnd="70dp"
        android:layout_marginBottom="11dp"
        android:background="@drawable/redcan"
        android:onClick="litterBtn"
        app:layout_constraintBottom_toTopOf="@+id/glassBtn"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageButton
        android:id="@+id/newsBtn"
        android:layout_width="206dp"
        android:layout_height="174dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="87dp"
        android:background="@drawable/newspaper"
        android:onClick="litterBtn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/bottleBtn" />

    <ImageButton
        android:id="@+id/glassBtn"
        android:layout_width="263dp"
        android:layout_height="143dp"
        android:layout_marginEnd="192dp"
        android:layout_marginBottom="36dp"
        android:background="@drawable/brokenbottle"
        android:onClick="litterBtn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>